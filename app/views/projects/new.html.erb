<div class="row">
  <div class="col-md-6 col-md-offset-3">
<%= form_with(model: @project,method: :post, local: true) do |f| %>
      <%= f.label :project_name %>
      <%= f.text_field :project_name, class: 'form-control' %>

      <%= f.label :status%>
      <%= f.number_field :Working_hours, class: 'form-control' %>

      <%= f.hidden_field :user_id, value:current_user.id %>

      <%= f.submit "Upload status", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>